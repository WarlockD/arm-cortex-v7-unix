#include "fs.h"
namespace {
	static constexpr size_t CACHE_SIZE = 64;
	using inode_cache_t = xv6::static_cache<xv6::inode,CACHE_SIZE>;
	using file_cache_t = xv6::static_cache<xv6::file,CACHE_SIZE>;
	static inode_cache_t inode_cache;
	static file_cache_t file_cache;
};
namespace xv6 {


} /* namespace xv6 */
