#ifndef _CLIST_HPP_
#define _CLIST_HPP_

#include <cstdint>
#include <cstddef>


namespace os {


	template<size_t _NCLIST, size_t _CBSIZE=16>
	class clist {
	public:
		constexpr static size_t CBSIZE = _CBSIZE;
		constexpr static size_t NCLIST = _NCLIST;
		struct cblock {
			cblock* _next;
			char info[]
		};
	};

};


#endif
