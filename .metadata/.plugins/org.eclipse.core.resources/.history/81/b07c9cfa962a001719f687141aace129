int uartputc(int c) {
	while(__HAL_UART_GET_FLAG(&huart1, UART_FLAG_TXE) == 0);
	huart1.Instance->TDR = c & 0xFF;
	while(__HAL_UART_GET_FLAG(&huart1, UART_FLAG_TC) == 0);
	return c & 0xFF;
}
